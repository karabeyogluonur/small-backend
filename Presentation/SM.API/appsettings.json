{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Common": {
    "Host": "https://example.com",
    "CDN": "https://cdn.example.com"
  },
  "ClientUrls": {
    "Base": "http://localhost:3000",
    "ForgotPassword": "http://localhost:3000/forgot-password"
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "SMDbConnection": "Server=localhost;Database=SampleDb;User Id=sample_user;Password=sample_password;"
  },
  "CORS": {
    "AllowedOrigins": "http://localhost:3000;http://localhost:8080;https://example-client.com;"
  },
  "JWT": {
    "Audience": "example.com",
    "Issuer": "example.com",
    "SecurityKey": "THIS_IS_A_DUMMY_JWT_SECURITY_KEY_CHANGE_IT",
    "AccessTokenExpirationTime": 180,
    "RefreshTokenExpirationTime": 360
  },
  "EmailAccount": {
    "Host": "smtp.example.com",
    "Port": 587,
    "Username": "dummy_user",
    "Password": "dummy_password",
    "EnableSsl": true
  }
}
